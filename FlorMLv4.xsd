<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:mods="http://www.loc.gov/mods/v3" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:import namespace="http://www.loc.gov/mods/v3" schemaLocation="http://www.loc.gov/standards/mods/v3/mods-3-1.xsd"/>
	<xs:element name="publication">
		<xs:annotation>
			<xs:documentation>publication, e.g. flora, fauna, taxonomic article</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="metaData"/>
				<xs:element name="textSection" type="textSectionType" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>for any text that is not part of a taxonomic treatment or addenda/errata/corrigenda, and not metadata</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="treatment" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>a taxonomic treatment</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="addenda" type="addendaType" minOccurs="0">
					<xs:annotation>
						<xs:documentation>addenda, errata, corrigenda</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
			<xs:attribute name="lang" type="xs:language">
				<xs:annotation>
					<xs:documentation>language of publication</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="defaultMediaUrl">
		<xs:annotation>
			<xs:documentation>base URL for illustrations</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="defaultMeasurementSystem">
		<xs:annotation>
			<xs:documentation>SI, Imperial, etc.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="metaData">
		<xs:annotation>
			<xs:documentation>metadata</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="metaDataType">
					<xs:choice maxOccurs="unbounded">
						<xs:element ref="defaultMediaUrl"/>
						<xs:element ref="defaultMeasurementSystem" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Use to define a default measurement system as used throughout the publication. SI, Imperial, etc.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:any namespace="http://www.loc.gov/mods/v3"/>
					</xs:choice>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="acceptedName">
		<xs:annotation>
			<xs:documentation>accepted name</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="nom" type="nomType" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="addendumPublication">
		<xs:annotation>
			<xs:documentation>to what publication is this an addendum?</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="refPart" type="refPartType" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>reference parts</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="addendumTaxon">
		<xs:annotation>
			<xs:documentation>to what taxon is this an addendum?</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="name" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="addendumOther">
		<xs:annotation>
			<xs:documentation>to what else is this an addendum? only use if addendum doesn't contain taxonomic information</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="alternativeCollectionTypeStatus">
		<xs:annotation>
			<xs:documentation>alternative collection type status</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="alternativeCollector">
		<xs:annotation>
			<xs:documentation>alternative collector</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="alternativeFieldNum">
		<xs:annotation>
			<xs:documentation>alternative number</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element name="typeNotes" type="notesType" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Notes on the type or the type status. DON'T put nomenclatural notes in here!</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
			<xs:attribute name="doubtful" type="xs:boolean"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="altitude">
		<xs:annotation>
			<xs:documentation>altitude</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0">
				<xs:element ref="br" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element name="references" type="referencesType" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element name="figureRef" type="figureRefType" minOccurs="0" maxOccurs="unbounded"/>
			</xs:choice>
			<xs:attribute name="estimate" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>if altitude is explicitly indicated to be estimate, set to true.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="range" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>set to true if instead of a single value a range is given</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="value">
		<xs:annotation>
			<xs:documentation>for numerical values in altitudes, characters</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:attribute name="class">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="mininum"/>
						<xs:enumeration value="average"/>
						<xs:enumeration value="median"/>
						<xs:enumeration value="maximum"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="b">
		<xs:annotation>
			<xs:documentation>text in bold</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="br">
		<xs:annotation>
			<xs:documentation>line break</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="vernacularNames" type="vernacularNamesType">
		<xs:annotation>
			<xs:documentation>vernacular names</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="vernacularName">
		<xs:annotation>
			<xs:documentation>one vernacular name</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:choice maxOccurs="unbounded">
				<xs:element name="refPart" type="refPartType" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="name" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>in this location always use with class="vernacular", one set of tags for each vernacular name.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="localLanguage" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>local language (for vernacular names). One set of tags per local language.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="translation" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="locality" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element name="footnoteRef" type="footnoteTypeRef" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="fullName" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>unatomized name</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="localLanguage">
		<xs:annotation>
			<xs:documentation>local language (for vernacular names)</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:attribute name="doubtful" type="xs:boolean"/>
			<xs:attribute name="unknown" type="xs:boolean"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="translation" type="xs:string">
		<xs:annotation>
			<xs:documentation>translation of a vernacular name into the base language of a taxonomic work</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="charClass">
		<xs:annotation>
			<xs:documentation>for any character classes not included in elements char and subChar</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="subChar" type="subCharType">
		<xs:annotation>
			<xs:documentation>sub-character</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="collector">
		<xs:annotation>
			<xs:documentation>collector</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:attribute name="doubtful" type="xs:boolean"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="couplet" type="coupletType">
		<xs:annotation>
			<xs:documentation>couplet</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="day" type="xs:gDay">
		<xs:annotation>
			<xs:documentation>day date (DD)</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="datepub">
		<xs:annotation>
			<xs:documentation>dates of publication</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence maxOccurs="unbounded">
				<xs:element name="string" type="stringType"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="distributionLocality">
		<xs:annotation>
			<xs:documentation>list item (locality) for distribution</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:sequence>
				<xs:element name="coordinates" type="coordinatesType" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="class" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="world"/>
						<xs:enumeration value="continent"/>
						<xs:enumeration value="oceanic region"/>
						<xs:enumeration value="continental region"/>
						<xs:enumeration value="country"/>
						<xs:enumeration value="department"/>
						<xs:enumeration value="province"/>
						<xs:enumeration value="subprovince"/>
						<xs:enumeration value="region"/>
						<xs:enumeration value="department"/>
						<xs:enumeration value="district"/>
						<xs:enumeration value="subdistrict"/>
						<xs:enumeration value="division"/>
						<xs:enumeration value="subdivision"/>
						<xs:enumeration value="locality"/>
						<xs:enumeration value="state"/>
						<xs:enumeration value="territory"/>
						<xs:enumeration value="kingdom"/>
						<xs:enumeration value="regency"/>
						<xs:enumeration value="county"/>
						<xs:enumeration value="town"/>
						<xs:enumeration value="tdwg1"/>
						<xs:enumeration value="tdwg2"/>
						<xs:enumeration value="tdwg3"/>
						<xs:enumeration value="tdwg4"/>
						<xs:enumeration value="other"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="status">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="absent"/>
						<xs:enumeration value="adventive"/>
						<xs:enumeration value="cultivated"/>
						<xs:enumeration value="endemic"/>
						<xs:enumeration value="escaped"/>
						<xs:enumeration value="expected"/>
						<xs:enumeration value="extinct"/>
						<xs:enumeration value="formerly cultivated"/>
						<xs:enumeration value="fossil"/>
						<xs:enumeration value="introduced"/>
						<xs:enumeration value="invasive"/>
						<xs:enumeration value="native"/>
						<xs:enumeration value="naturalized"/>
						<xs:enumeration value="possible"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="frequency">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="absent"/>
						<xs:enumeration value="very rare"/>
						<xs:enumeration value="rare"/>
						<xs:enumeration value="common"/>
						<xs:enumeration value="extensive"/>
						<xs:enumeration value="uncommon"/>
						<xs:enumeration value="widespread"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="doubtful" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>true if there is some doubt about the occurrence in this area or about the status. May also be indicated by using an ? in the distribution text. Example: "?Sumatra"</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="extra" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>True, if this disbributionLocality has been added additionally to the text, indicating that the distribution text is not part of the original text. This is sometimes necessary if e.g. an area is mentioned only partly within an enumeration or the way how it needs to be parsed is not in accordance the original text. 
Example: West, East and North Africa. If you want to markup West Africa as a separate region this is not possible, because "North" is not a unique area name. Instead you do: North <distribtuionLocality extra="true">North Africa</distribtuionLocality>, South ...</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="distributionScope">
		<xs:annotation>
			<xs:documentation>for scopes in distributions containing one or more distribution localities</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:sequence>
				<xs:element ref="distributionLocality" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>distribution locality. Only valid in feature[class="distribution"] context</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="class" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="world"/>
						<xs:enumeration value="continent"/>
						<xs:enumeration value="oceanic region"/>
						<xs:enumeration value="continental region"/>
						<xs:enumeration value="country"/>
						<xs:enumeration value="department"/>
						<xs:enumeration value="province"/>
						<xs:enumeration value="subprovince"/>
						<xs:enumeration value="region"/>
						<xs:enumeration value="district"/>
						<xs:enumeration value="subdistrict"/>
						<xs:enumeration value="locality"/>
						<xs:enumeration value="state"/>
						<xs:enumeration value="county"/>
						<xs:enumeration value="town"/>
						<xs:enumeration value="tdwg1"/>
						<xs:enumeration value="tdwg2"/>
						<xs:enumeration value="tdwg3"/>
						<xs:enumeration value="tdwg4"/>
						<xs:enumeration value="other"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="doubtful" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>true if there is some doubt about the occurrence in this area or about the status. May also be indicated by using an ? in the distribution text. Example: "?Sumatra"</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="feature">
		<xs:annotation>
			<xs:documentation>any feature (e.g. description, distribution, geography, ecology, notes, etc.)</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element name="heading" type="headingType" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element name="writer" type="writerType" minOccurs="0"/>
				<xs:element ref="num" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element name="char" type="charType" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>parent character</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="habitatList" type="habitatListType" minOccurs="0"/>
				<xs:element name="string" type="stringType" minOccurs="0">
					<xs:annotation>
						<xs:documentation>any text that isn't a character or isn't covered by one of the other elements</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="figureRef" type="figureRefType" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element name="figure" type="figureType" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="literatureRef" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element name="references" type="referencesType" minOccurs="0"/>
				<xs:element name="key" type="keyType" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>for keys that are part of a feature</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
			<xs:attribute name="isFreetext" type="xs:boolean" default="false">
				<xs:annotation>
					<xs:documentation>Certain features (e.g. distribution or vernacular names) are usually handled as structured data. If the feature is explicitly not structured the isFreetext attribute should be set to true</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="lang" type="xs:language">
				<xs:annotation>
					<xs:documentation>language of feature</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="class" use="required">
